                        </asp:MenuItem>
                        <asp:MenuItem NavigateUrl="~/Administracion/Reportes.aspx" 
                            Text="Comité Paritario" Value="Comité Paritario"></asp:MenuItem>
                    </Items>
                    <StaticMenuItemStyle BackColor="#003366" BorderColor="White" 
                        BorderStyle="Solid" BorderWidth="1px" ForeColor="White" HorizontalPadding="4px" 
                        ItemSpacing="4px" VerticalPadding="4px" />
                    <StaticMenuStyle HorizontalPadding="4px" VerticalPadding="4px" />
                </asp:Menu>
                        </td>
                        <td>
                            <asp:Label ID="lblNombre" runat="server" ForeColor="White"></asp:Label>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
        <div class="main">
            <asp:ContentPlaceHolder ID="MainContent" runat="server"/>
        </div>
        <div class="clear">
        </div>
    </div>
    <div class="footer">
        
    </div>
    </form>
