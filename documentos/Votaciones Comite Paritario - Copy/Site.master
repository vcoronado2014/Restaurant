                                IncludeStyleBlock="false" Orientation="Horizontal" Font-Bold="True" 
                                ForeColor="White">
                    <DynamicHoverStyle ForeColor="White" />
                    <DynamicMenuItemStyle ForeColor="White" />
                    <DynamicSelectedStyle ForeColor="White" />
                    <Items>
                        <asp:MenuItem NavigateUrl="~/Default.aspx" Text="Inicio"/>
                        <asp:MenuItem NavigateUrl="~/Documentos/Ley.aspx" Text="Infórmese sobre la Ley" 
                            Value="Infórmese sobre la Ley"></asp:MenuItem>
                        <asp:MenuItem NavigateUrl="~/Documentos/Documentos.aspx" Text="Documentos" 
                            Value="Documentos"></asp:MenuItem>
                        <asp:MenuItem NavigateUrl="~/Votaciones/SolicitudKey.aspx" 
